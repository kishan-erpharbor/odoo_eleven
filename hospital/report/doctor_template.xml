<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="report_doctor">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>Doctor Report</h2>
                        <p>This object's name is
                            <span t-field="o.name"/>
                        </p>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
